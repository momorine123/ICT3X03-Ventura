pipeline {
agent any
stages {
stage ('Checkout') {
steps {
git branch:'main', url: 'https://github.com/momorine123/ICT3X03-Ventura.git'
}
}
stage('Code Quality Check via SonarQube') {
steps {
script {
def scannerHome = tool 'SonarQube';
withSonarQubeEnv('SonarQube') {
sh "${scannerHome}/bin/sonar-scanner \
   -Dsonar.projectKey=OWASP \
  -Dsonar.sources=. \
  -Dsonar.host.url=http://192.168.1.133:9000 \
  -Dsonar.login=sqp_82a83af9c2dcee068b6653239c3ddae458aa110b"
}
}
}
}
}
}